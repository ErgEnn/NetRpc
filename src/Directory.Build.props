<Project>
  <PropertyGroup>
    <Authors>ShadowK</Authors>
    <Version>1.0.36</Version>
    <Description>NetRpc is a light weight rpc engine base on RabbitMq or Grpc targeting .NET Standard. It use the simple interface to call each other, contains the load balance mode, middleware.</Description>
    <PackageProjectUrl>https://github.com/newshadowk/NetRpc</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryRoot>$(MSBuildThisFileDirectory)</RepositoryRoot>
    <RepositoryUrl>https://github.com/newshadowk/NetRpc</RepositoryUrl>
     <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
</Project>
