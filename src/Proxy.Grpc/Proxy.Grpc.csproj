    <AssemblyOriginatorKeyFile>NetRpc.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.8.0" />
    <PackageReference Include="Grpc" Version="1.21.0" />
  </ItemGroup>


  <ItemGroup>
    <Folder Include="Message\" />
  </ItemGroup>

</Project>
